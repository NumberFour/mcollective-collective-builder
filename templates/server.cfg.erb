topicprefix = /topic/<%= collective %>
libdir = <%= instance_home %>/plugins
logfile = <%= BASEDIR %>/logs/<%= identity %>-server.log
loglevel = debug
daemonize = 1
identity = <%= identity %>

# Plugins
securityprovider = none

connector = stomp
plugin.stomp.host = <%= stompserver %>
plugin.stomp.port = <%= stompport %>
plugin.stomp.user = <%= stompuser %>
plugin.stomp.password = <%= stomppass %>

factsource = yaml
plugin.yaml = <%= instance_home %>/etc/facts.yaml

classesfile = <%= instance_home %>/etc/classes.txt
